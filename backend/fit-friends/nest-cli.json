{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fit-friends/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/fit-friends/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/fit-friends",
  "projects": {
    "fit-friends": {
      "type": "application",
      "root": "apps/fit-friends",
      "entryFile": "main",
      "sourceRoot": "apps/fit-friends/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fit-friends/tsconfig.app.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    },
    "shared/app-types": {
      "type": "library",
      "root": "libs/shared/app-types",
      "entryFile": "index",
      "sourceRoot": "libs/shared/app-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/app-types/tsconfig.lib.json"
      }
    }
  }
}